{
  "name": "@niteco/utils-server",
  "version": "1.0.0",
  "description": "Shared utilities for Niteco packages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "keywords": [
    "niteco",
    "utilities",
    "config"
  ],
  "author": "Niteco",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {},
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "require": "./dist/config.js",
      "types": "./dist/config.d.ts"
    }
  }
}
